[Unit]
Description=Python transcript Script Service
After=multi-user.target

[Service]
Type=simple
ExecStart=/usr/bin/python3 /home/ubuntu/hark-python-transcript/audio_transcript.py > /home/ubuntu/hark-python-transcript/audio_transcript.log &
User=root
Restart=always

[Install]
WantedBy=multi-user.target
